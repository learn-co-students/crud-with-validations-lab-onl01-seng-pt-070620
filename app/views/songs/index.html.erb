<h1>Songs</h1>
<table>
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Genre</th>
        <th>Artist name</th>
        <th></th>
    </tr>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.release_year %></td>
                <td><%= song.genre %></td>
                <td><%= song.artist_name %></td>
                <td>
                <td>
                    <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>