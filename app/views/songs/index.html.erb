<h1>All Songs</h1>

<% @songs.each  do |song| %>
    <div><%= link_to song.title, song_path(song) %></div>    
    <% end %>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Genre</th>
            <th>Artist name</th>
            <th></th>
        <tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.release_year %></td>
                <td><%= song.genre %></td>
                <td><%= song.artist_name %></td>
                <td>
                    <%= link_to 'View', song_path(song) %> |
                    <%= link_to 'Edit', edit_song_path(song) %> |
                    <%= button_to "Delete", song_path(song), method: :delete %> 

                </td>
            </tr> 
        <% end %> 
    </tbody>
</table>
    