<div class="songSolo">
  <% if @song.errors.any? %>
    <div class="error_explained">
      <% @song.errors.full_messages.each do |msg| %>
        <%= msg %>
      <% end %>
    </div>
  <% end %>
  <ul>
    <li>
      <% if @song.artist_name? %>
        <div class="songImg"><img src="<%= grab_art(@song) %>"></div>
      <% end %>
    </li>
    <% if @song.title? %> <div class="songtitle"><%= @song.title %> </div> <% end %>
    <li>
      <%= form_for @song do |f| %>
        Title: <%= f.text_field :title %><br>
        Artist: <%= f.text_field :artist_name %><br>
        Genre: <%= f.text_field :genre %><br>
        Release Year: <%= f.text_field :release_year %><br>
        Released?: <%= f.check_box :released %><br>
        <%= f.submit %>
      <% end %>
    </li>
  </ul>
</div>